#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/xss@1.0.8/node_modules/xss/bin/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/xss@1.0.8/node_modules/xss/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/xss@1.0.8/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules:/home/peter/Desktop/Projects/saathi/node_modules:/home/peter/Desktop/Projects/node_modules:/home/peter/Desktop/node_modules:/home/peter/node_modules:/home/node_modules:/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/node_modules/xss/bin/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/node_modules/xss/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../xss/bin/xss" "$@"
else
  exec node  "$basedir/../xss/bin/xss" "$@"
fi
