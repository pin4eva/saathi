#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/micro@9.3.4/node_modules/micro/bin/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/micro@9.3.4/node_modules/micro/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/micro@9.3.4/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/.pnpm/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules:/home/peter/Desktop/Projects/saathi/node_modules:/home/peter/Desktop/Projects/node_modules:/home/peter/Desktop/node_modules:/home/peter/node_modules:/home/node_modules:/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/micro/bin/node_modules:/home/peter/Desktop/Projects/saathi/server/node_modules/micro/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../micro/bin/micro.js" "$@"
else
  exec node  "$basedir/../micro/bin/micro.js" "$@"
fi
